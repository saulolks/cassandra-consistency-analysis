          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import com.datastax.driver.core.Cluster;
import com.datastax.driver.core.Session;
import com.datastax.driver.core.QueryOptions;
import com.datastax.driver.core.ConsistencyLevel;

Cluster cluster = null
if(vars.get(&quot;consistency&quot;) == &quot;ONE&quot;) {
	cluster = Cluster.builder()
		.addContactPoint(&quot;localhost&quot;)
		.withQueryOptions(
			new QueryOptions()
	    		.setConsistencyLevel(ConsistencyLevel.ONE)
	    	)
		.withPort(9042).build();
}
else if(vars.get(&quot;consistency&quot;) == &quot;QUORUM&quot;){
	cluster = Cluster.builder()
		.addContactPoint(&quot;localhost&quot;)
		.withQueryOptions(
			new QueryOptions()
	    		.setConsistencyLevel(ConsistencyLevel.QUORUM)
	    	)
		.withPort(9042).build();
}
else {
	cluster = Cluster.builder()
		.addContactPoint(&quot;localhost&quot;)
		.withQueryOptions(
			new QueryOptions()
	    		.setConsistencyLevel(ConsistencyLevel.ALL)
	    	)
		.withPort(9042).build();
}


Session session = cluster.connect(&quot;testvalidation&quot;);

props.put(&quot;cluster&quot;, cluster);
props.put(&quot;session&quot;, session);</stringProp>
        </JSR223Sampler>
